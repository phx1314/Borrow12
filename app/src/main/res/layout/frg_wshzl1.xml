<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical"
    android:paddingLeft="@dimen/j12dp"
    android:paddingRight="@dimen/j12dp">

    <EditText
        android:id="@+id/mEditText_1"
        style="@style/text_style_15_black"
        android:layout_width="match_parent"
        android:layout_height="@dimen/j60dp"
        android:layout_marginTop="@dimen/j10dp"
        android:background="#00000000"
        android:gravity="center_vertical"
        android:hint="姓名"
        android:padding="@dimen/j5dp"
        android:singleLine="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/tyD1" />

    <EditText
        android:id="@+id/mEditText_2"
        style="@style/text_style_15_black"
        android:layout_width="match_parent"
        android:layout_height="@dimen/j60dp"
        android:background="#00000000"
        android:gravity="center_vertical"
        android:hint="身份证"
        android:padding="@dimen/j5dp"
        android:singleLine="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/tyD1" />

    <EditText
        android:id="@+id/mEditText_zmf"
        style="@style/text_style_15_black"
        android:layout_width="match_parent"
        android:layout_height="@dimen/j60dp"
        android:background="#00000000"
        android:gravity="center_vertical"
        android:hint="芝麻分"
        android:padding="@dimen/j5dp"
        android:singleLine="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/tyD1" />

    <TextView
        android:id="@+id/mEditText_3"
        style="@style/text_style_15_black"
        android:layout_width="match_parent"
        android:layout_height="@dimen/j60dp"
        android:background="#00000000"
        android:gravity="center_vertical"
        android:hint="性别"
        android:padding="@dimen/j5dp"
        android:singleLine="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/tyD1" />

    <TextView
        android:id="@+id/mEditText_4"
        style="@style/text_style_15_black"
        android:layout_width="match_parent"
        android:layout_height="@dimen/j60dp"
        android:background="#00000000"
        android:gravity="center_vertical"
        android:hint="年龄"
        android:inputType="number"
        android:padding="@dimen/j5dp"
        android:singleLine="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/tyD1" />
</LinearLayout>

